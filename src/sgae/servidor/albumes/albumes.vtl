<html>
<head><title> Ejemplo AlbumInfoBreve </title></head>
<body>
	<table>
		<tbody>
			#foreach($album in $albumes)
			<tr>
				<td>Id Album</td><td>$album.idAlbum</td>
			</tr>
			<tr>
				<td>Titulo</td><td>$album.titulo</td>
			</tr>
			<tr>
				<td>Fecha de publicacion</td><td>$album.fechaPublicacion</td>
			</tr>
			<tr>
				<td>Ejemplares vendidos</td><td>$album.ejemplaresVendidos</td>
			</tr>
			#end
		</tbody>
	</table>
</body>
</html>